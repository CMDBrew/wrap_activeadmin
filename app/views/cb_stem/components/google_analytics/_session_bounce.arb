panel t('cb_stem.components.google_analytics.session.bounce') do
  result_hash = {}

  results =
    CbStem::GoogleAnalytics.results(
      property,
      start_date: start_date,
      end_date:   end_date
    )
  results.metrics << 'bounceRate'

  value = results.first&.bounceRate

  if value.present?
    h1 to_percentage(value), class: 'display-1 mb-4'
  else
    cb_stem_component('charts/empty_state')
  end
end
